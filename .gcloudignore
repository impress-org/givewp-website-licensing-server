/.prettierrc.json
/.huskyrc.json
/commitlint.config.js
/package.json
/yarn.lock
/phpcs.xml
/phpunit.xml
/.styleci.yml

# Laravel & Test files
/storage
/tests
/.env
/.env.testing
/.env.example

# Composer files
/vendor

# NPM Packages
node_modules/

